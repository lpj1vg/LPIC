        </mxCell>
        <mxCell id="0ogWrCx-0VYVaQKFf6Zu-36" value="&lt;h3 style=&quot;text-align: center&quot;&gt;CÓMO AUMENTAR LA SWAP&lt;/h3&gt;&lt;div&gt;# Comprobamos espacio disponible con &#39;pvs&#39;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;br&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;swapoff -a&lt;/span&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;lvresize -L4G /dev/mapper/vgsys-swap&lt;/div&gt;&lt;div&gt;mkswap /dev/mapper/vgsys-swap&lt;/div&gt;&lt;div&gt;swapon -a&lt;/div&gt;" style="text;resizable=0;autosize=1;align=left;verticalAlign=middle;points=[];fillColor=#b0e3e6;rounded=0;shadow=0;glass=0;sketch=0;fontFamily=Lucida Console;html=1;fontSize=12;spacingLeft=12;spacingBottom=6;spacing=0;spacingTop=-6;spacingRight=0;" vertex="1" parent="0ogWrCx-0VYVaQKFf6Zu-1">
